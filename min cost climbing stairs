class Solution {
public:
   
    int minCostClimbingStairs(vector<int>& cost) {
        
         int n=cost.size();
        
         vector<int>dp(n+1,0);
        
         for(int i=2;i<=n;i++){
             
              int p1=dp[i-1]+cost[i-1];
              int p2=dp[i-2]+cost[i-2];
             
              dp[i]=min(p1,p2);
             
         }
        return dp[n];
    }
};
